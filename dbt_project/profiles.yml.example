# Example profiles.yml for dbt-duckdb (place a copy at ~/.dbt/profiles.yml)

kueski:
  target: dev
  outputs:
    dev:
      type: duckdb
      path: dev.db
      threads: 1
      extensions: ['parquet']

# To run dbt locally with the CSV/parquet files produced by ingestion set the env var:
# PROCESSED_DIR=data/processed
# Then run:
# dbt run --project-dir dbt_project
